  "trap_cards": [
    {
      "name": "Fuel Thief",
      "description": "Steals Fuel from opponent. Amount depends on target's class. Reveals one of your deployed trucks.",
      "effects": [
        {
          "type": "steal_fuel",
          "target": "opponent_truck",
          "amounts": [
            { "class": "Heavy", "value": 200 },
            { "class": "Medium", "value": 150 },
            { "class": "Light", "value": 100 }
          ]
        },
        {
          "type": "reveal_own_truck",
          "target": "self_deployed_truck_random"
        }
      ]
    },
    {
      "name": "Breakdown",
      "description": "Immobilizes an opponent's truck for next turn, and it cannot be attacked. Costs 100 of your Fuel.",
      "effects": [
        {
          "type": "immobilize_truck",
          "target": "opponent_truck"
        },
        {
          "type": "lose_fuel",
          "target": "self",
          "value": 100
        }
      ]
    },
    {
      "name": "Police",
      "description": "Destroys any attacking truck if it's hidden. Reveals all your deployed trucks.",
      "effects": [
        {
          "type": "destroy_truck",
          "target": "opponent_truck_hidden"
        },
        {
          "type": "reveal_all_self",
          "target": "all_trucks"
        }
      ]
    }
  ]
